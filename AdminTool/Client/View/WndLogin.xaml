<Window x:Class="AdminTool.Client.View.WndLogin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WndLogin" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="tbUsername" HorizontalAlignment="Left" Height="23" Margin="67,38,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tbPassword" HorizontalAlignment="Left" Height="23" Margin="67,66,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnLogin" Content="Button" HorizontalAlignment="Left" Margin="84,105,0,0" VerticalAlignment="Top" Width="75" Click="btnLogin_Click"/>

    </Grid>
</Window>
